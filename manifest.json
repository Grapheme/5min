{
  "manifest_version": 2,

  "name": "fb5min",
  "description": "Экстеншин против прокрастинации.",
  "version": "0.1",
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "tabs", "https://*/*", "http://*/*"
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html",
    "default_title": "Это тултип!"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
      {
          "matches": ["http://*/*", "https://*/*"],
          "js": [
            "js/vendor/jquery.min.js",
            "js/content.js"
          ]
      }
  ]
}